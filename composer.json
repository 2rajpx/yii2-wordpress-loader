{
    "name": "amarshimi/yii2-wordpress-loader",
    "version": "2.0",
    "description": "Use wordpress with yii2",
    "keywords": ["yii2", "wordpress"],
    "type": "yii2-extension",
    "license": "Commercial",
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "johnpbloch/wordpress": "4.2"
    },
    "autoload": {
        "psr-4": {
            "amarshimi\\wordpress\\loader\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "wordpress-install-dir": "wordpress"
    }
}
